{% extends 'FrontBundle:Default:index.html.twig'%}
{%block ubicacion%}
<a href="{{path('proveedor')}}">Iniciar sesion</a> <div class="breadcrumb_divider"> <!--</div><a class="current">Editar</a>-->
{%endblock ubicacion%}
{%block id 'usuario'%}
{% block cuerpo1 %}
    {% if error %}
        <div>{{ error.message }}</div>
    {% endif %}
        <form action="{{ path('usuario_login_check') }}" method="post">
        <label for="username">Usuario:</label>
        <input type="text" id="username" name="_username"
        value="{{ last_username }}" />
        <label for="password">Contraseña:</label>
        <input type="password" id="password" name="_password" />
        <input type="checkbox" id="no_cerrar" name="_remember_me" checked />
        <label for="no_cerrar">No cerrar sesión</label>
        <input type="submit" name="login" value="Acceder" />
        </form>
    {% endblock %}
{% block aside %}{% endblock %}
